@Model.ImeStudenta
<br />
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Predmet</th>
            <th>Datum</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in  Model.Zapisi)
        {
            <tr>
                <td>@x.Predmet</td>
                <td>@x.Datum</td>
            </tr>
        }
    </tbody>
</table>