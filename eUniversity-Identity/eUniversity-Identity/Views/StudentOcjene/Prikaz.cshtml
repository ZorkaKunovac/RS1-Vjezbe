
    @using Data.EntityModels
    @model List<StudentOcjenePrikazVM>

    @{
        ViewData["Title"] = "Prikaz";
        Layout = null;
    }

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Naziv predmeta</th>
                <th>Ocjena</th>
                <th>Datum</th>
                <th>Akcija</th>
            </tr>
        </thead>
        <tbody>
            @foreach (StudentOcjenePrikazVM s in Model)
            {
            <tr>
                <td>@s.NazivPredmeta</td>
                <td>@s.BrojcanaOcjena</td>
                <td>@s.Datum.ToString("dd.MM.yyyy") </td>
                <td>
                    <button onclick="OcjeneUredi(@s.OcjenaID)">Uredi</button>
                </td>
            </tr>
            }
        </tbody>
    </table>


